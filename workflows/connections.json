{
    "managedApiConnections": {
        "sap": {
            "api": {
                "id": "/subscriptions/@appsetting('SubscriptionId')/providers/Microsoft.Web/locations/westeurope/managedApis/sap"
            },
            "authentication": {
                "type": "ManagedServiceIdentity"
            },
            "connection": {
                "id": "/subscriptions/@appsetting('SubscriptionId')/resourceGroups/portabilityWE-rg/providers/Microsoft.Web/connections/sap-portability"
            },
            "connectionRuntimeUrl": "@appsetting('SapConnectionRuntimeUrl')"
        }
    },
    "serviceProviderConnections": {
        "keyVault": {
            "displayName": "portability_client_connection",
            "parameterValues": {
                "VaultUri": "@appsetting('KeyVaultUri')",
                "authProvider": {
                    "Type": "ManagedServiceIdentity"
                }
            },
            "serviceProvider": {
                "id": "/serviceProviders/keyVault"
            }
        }
    }
}
